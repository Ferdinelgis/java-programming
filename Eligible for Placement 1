import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String a=s.nextLine();
        System.out.println("Name of the Student:"+a);
        int b=s.nextInt();
        int c=s.nextInt();
        if(b==1){
            if(c>=70){
                System.out.println(a+" is Eligible for Placement");
            }
            else{
                System.out.println(a+" is Not Eligible for Placement");
            } }
        else if((b==1 )|(b==2)){
            if(c>=75){
                System.out.println(a+" is Eligible for Placement");
            }
            else{
                System.out.println(a+" is Not Eligible for Placement");
            }
        }
        else{
           System.out.println(a+" is Not Eligible for Placement");
            }
       
           
        }
    }
